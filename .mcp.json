{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp"]
    },
    "github": {
      "command": "npx",
      "args": ["-y", "github-mcp"]
    },
    "shadcn": {
      "command": "npx",
      "args": [
        "shadcn@latest",
        "mcp"
      ]
    },
    "acternity-ui": {
      "command": "npx",
      "args": [
        "-y",
        "@acternity-ui/mcp"
      ]
    }
    
  }
}
